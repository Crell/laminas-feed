{
    "name": "laminas/laminas-feed",
    "description": "provides functionality for consuming RSS and Atom feeds",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "feed"
    ],
    "homepage": "https://laminas.dev",
    "support": {
        "docs": "https://docs.laminas.dev/laminas-feed/",
        "issues": "https://github.com/laminas/laminas-feed/issues",
        "source": "https://github.com/laminas/laminas-feed",
        "rss": "https://github.com/laminas/laminas-feed/releases.atom",
        "chat": "https://laminas.dev/chat",
        "forum": "https://discourse.laminas.dev"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.2-dev",
            "dev-develop": "2.3-dev"
        }
    },
    "require": {
        "php": ">=5.3.3",
        "laminas/laminas-escaper": "self.version",
        "laminas/laminas-servicemanager": "self.version",
        "laminas/laminas-stdlib": "self.version",
        "laminas/laminas-uri": "self.version",
        "laminas/laminas-zendframework-bridge": "^1.0",
        "zendframework/zend-version": "self.version"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "1.7.*",
        "laminas/laminas-cache": "self.version",
        "laminas/laminas-db": "self.version",
        "laminas/laminas-http": "self.version",
        "laminas/laminas-servicemanager": "self.version",
        "laminas/laminas-validator": "self.version",
        "phpunit/phpunit": "~4.0",
        "satooshi/php-coveralls": "dev-master"
    },
    "suggest": {
        "laminas/laminas-cache": "Laminas\\Cache component",
        "laminas/laminas-db": "Laminas\\Db component",
        "laminas/laminas-http": "Laminas\\Http for PubSubHubbub, and optionally for use with Laminas\\Feed\\Reader",
        "laminas/laminas-servicemanager": "Laminas\\ServiceManager component, for default/recommended ExtensionManager implementations",
        "laminas/laminas-validator": "Laminas\\Validator component"
    },
    "autoload": {
        "psr-4": {
            "Laminas\\Feed\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaminasTest\\Feed\\": "test/"
        }
    },
    "replace": {
        "zendframework/zend-feed": "self.version"
    }
}
